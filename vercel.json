{
  "version": 2,
  "name": "factory-dashboard-system",
  "builds": [
    {
      "src": "html_dashboards/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/html_dashboards/index.html"
    },
    {
      "src": "/dashboard",
      "dest": "/html_dashboards/recycled_items_enhanced_dashboard.html"
    },
    {
      "src": "/truck-dashboard",
      "dest": "/html_dashboards/thai_truck_dashboard.html"
    },
    {
      "src": "/sales-log",
      "dest": "/html_dashboards/recycled_items_sales_log.html"
    },
    {
      "src": "/purchase-log",
      "dest": "/html_dashboards/recycled_items_purchase_log.html"
    },
    {
      "src": "/sales-payment",
      "dest": "/html_dashboards/sales_payment_dashboard.html"
    },
    {
      "src": "/(.*)",
      "dest": "/html_dashboards/$1"
    }
  ],
  "headers": [
    {
      "source": "**/*.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "**/*.css",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "**/*.json",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache"
        }
      ]
    }
  ]
}
